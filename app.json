{
  "expo": {
    "name": "medicure App",
    "slug": "medicure-App",
    "scheme": "your-app-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "https://res.cloudinary.com/deohymauz/image/upload/v1704457322/100_Light_settings_invited_friends_2_n4lgbj.png",
      "resizeMode": "contain",
      "backgroundColor": "#4942E4"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "Permissions.LOCATION"
      ],
      "package": "com.sumilsuthar197.medicureAppDemo"
    },
    "androidStatusBarColor": "#4942E4",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#4942E4",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow medicure to use your location."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d3e24cb2-5079-4ab2-8711-009d30139fef"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d3e24cb2-5079-4ab2-8711-009d30139fef"
    }
  }
}
