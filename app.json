{
  "expo": {
    "name": "Medicure",
    "slug": "medicure-App",
    "scheme": "your-app-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "https://res.cloudinary.com/deohymauz/image/upload/v1704457322/100_Light_settings_invited_friends_2_n4lgbj.png",
      "resizeMode": "contain",
      "backgroundColor": "#4942E4"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "Permissions.LOCATION"
      ],
      "package": "com.sumilsuthar197.medicureAppDemo",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCJd2JfeBR0Tft2E51ltbV4qRVJw23NBEU"
        }
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#4942E4",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow medicure to use your location."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c3c84d73-eabe-415c-af3b-4cbb3374cbb3"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
